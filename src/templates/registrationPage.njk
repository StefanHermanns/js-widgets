<div class="wsb-content">
  <div class="wsb-page wsb-registration-page">
    <div class="wsb-header">
      <h1>{{ event.title }}</h1>
    </div>

    <div class="wsb-body">
      <div class="wsb-toolbar wsb-first">
        <div class="wsb-info-block">
          {% from "./event/info.njk" import info %}
          {{ info(event, config, _t, _f) }}
        </div>
      </div>

      <div class="wsb-description">
        {% from "./form.njk" import registrationForm %}
        {% if event.registrationForm %}
          {{ registrationForm(event, countries, config, _t, _f) }}
        {% endif %}
      </div>
    </div>
  </div>
</div>
