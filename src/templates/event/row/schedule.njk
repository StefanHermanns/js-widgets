{% macro renderSchedule(event, config, _t, _f) %}
  <div class="wsb-table__col wsb-table__col-schedule">
    {% if event.schedule.atOneDay() %}
      {{ _f(event.schedule, 'full_long') }}
      {% if event.location.online and event.schedule.timezone %}
        {{ _f(event.schedule, 'timezone_short') }}
      {% endif %}
    {% else %}
      {{ _f(event.schedule, 'full_short') }}
    {% endif %}
    {% if event.free %}
      <div class="wsb-free">{{ _t('schedule.event.free') | lower }}</div>
    {% endif %}
  </div>
{% endmacro %}
