{% from "./event/tag.njk" import tag %}
{% for event in events %}
  <div class="wsb-workshop {% if config.featured.highlight and event.featured %} wsb-featured {% endif %}">
    {{ tag(event, config, _t, _f, false) }}
    {{ _f(event.schedule, 'full_short') }} in {{ _f(event.location) }}
      <br>
      <a href="{{ event.url }}">
        {{ event.title }}
      </a>
  </div>
{% endfor %}

<div class="powered-by-wsb">
  <div class="powered-by-wsb-inner">
    <div>Powered by Workshop Butler!</div>
    <a href="/">Learn more</a>
  </div>

  <img src="data:image/png;base64," alt="Powered by Workshop Butler" />
</div>
